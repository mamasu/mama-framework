{
    "name": "mmf/framework",
    "description": "The Mamasu framework",
    "authors": [
        {
            "name": "Xavier Casahuga",
            "email": "xavier@mamasu.es"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://repo.mamasulabs.com/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "mmf/parameter": "0.0.*",
        "mmf/event": "0.0.*",
        "mmf/log": "0.0.*",
        "mmf/autoloader": "0.0.*",
        "twig/twig":"~1.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Mmf\\": "src/"
        },
	"exclude-from-classmap": ["/test/", "/app/", "/config/", "/mktfilesystem/", "/mmffilesystem/"]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": ["/test/", "/app/", "/config/", "/mktfilesystem/", "/mmffilesystem/"]
    },
    "minimum-stability": "dev"
}
